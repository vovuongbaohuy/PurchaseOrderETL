	poh.PurchaseOrderID,
	p.ProductID,
	v.BusinessEntityID as VendorID,
	sm.ShipMethodID,
	(YEAR(poh.OrderDate) * 10000 + MONTH(poh.OrderDate) * 100 + DAY(poh.OrderDate)) AS OrderDateID,
