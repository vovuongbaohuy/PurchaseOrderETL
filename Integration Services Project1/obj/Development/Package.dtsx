                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    (YEAR(OrderDate) * 10000 + MONTH(OrderDate) * 100 + DAY(OrderDate)) AS ID,
    DAY(OrderDate) AS Day,
    MONTH(OrderDate) AS Month,
    CASE 
        WHEN MONTH(OrderDate) BETWEEN 1 AND 3 THEN 1
        WHEN MONTH(OrderDate) BETWEEN 4 AND 6 THEN 2
        WHEN MONTH(OrderDate) BETWEEN 7 AND 9 THEN 3
        WHEN MONTH(OrderDate) BETWEEN 10 AND 12 THEN 4
    END AS Quarter,
    YEAR(OrderDate) AS Year
FROM Purchasing.PurchaseOrderHeader poh
WHERE poh.ModifiedDate &gt; (SELECT * FROM 
[MD PurchaseOrderHeaderDimMaxModifiedDate])
GROUP BY OrderDate

UNION

SELECT
	(YEAR(ShipDate) * 10000 + MONTH(ShipDate) * 100 + DAY(ShipDate)) AS ID,
	DAY(ShipDate) AS Day,
	MONTH(ShipDate) AS Month,
	CASE 
        WHEN MONTH(ShipDate) BETWEEN 1 AND 3 THEN 1
        WHEN MONTH(ShipDate) BETWEEN 4 AND 6 THEN 2
        WHEN MONTH(ShipDate) BETWEEN 7 AND 9 THEN 3
