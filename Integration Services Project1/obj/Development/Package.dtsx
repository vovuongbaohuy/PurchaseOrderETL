        WHEN MONTH(DueDate) BETWEEN 7 AND 9 THEN 3
        WHEN MONTH(DueDate) BETWEEN 10 AND 12 THEN 4
    END AS Quarter,
	YEAR(DueDate) AS Year
FROM Purchasing.PurchaseOrderDetail pod
WHERE pod.ModifiedDate &gt; ( SELECT * FROM 
[MD PurchaseOrderDetailDimMaxModifiedDate])
GROUP BY DueDate

