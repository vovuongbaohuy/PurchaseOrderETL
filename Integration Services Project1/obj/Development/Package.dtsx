                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	poh.PurchaseOrderID,
	p.ProductID,
	v.BusinessEntityID as VendorID,
	sm.ShipMethodID,
	(YEAR(poh.OrderDate) * 10000 + MONTH(poh.OrderDate) * 100 + DAY(poh.OrderDate)) AS OrderDateID,
	(YEAR(poh.ShipDate) * 10000 + MONTH(poh.ShipDate) * 100 + DAY(poh.ShipDate)) AS ShipDateID,
	(YEAR(pod.DueDate) * 10000 + MONTH(pod.DueDate) * 100 + DAY(pod.DueDate)) AS DueDateID,
	pod.UnitPrice,
	pod.OrderQty,
	pod.UnitPrice * pod.OrderQty AS Total
FROM Purchasing.PurchaseOrderHeader poh
FULL OUTER JOIN Purchasing.PurchaseOrderDetail pod
ON poh.PurchaseOrderID = pod.PurchaseOrderID
FULL OUTER JOIN Production.Product p
ON pod.ProductID = p.ProductID
FULL OUTER JOIN Purchasing.Vendor v
ON poh.VendorID = v.BusinessEntityID
FULL OUTER JOIN Purchasing.ShipMethod sm
ON poh.ShipMethodID = sm.ShipMethodID
