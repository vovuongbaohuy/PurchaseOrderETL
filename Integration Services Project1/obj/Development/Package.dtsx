OR v.ModifiedDate &gt; (SELECT * FROM [MD VendorDimMaxModifiedDate])
OR sm.ModifiedDate &gt; (SELECT * FROM [MD ShipDimMaxModifiedDate])
OR p.ModifiedDate &gt; (SELECT * FROM [MD ProductDimMaxModifiedDate])
